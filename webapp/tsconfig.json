{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": "src",
    "paths": {
      "@auth/*": ["auth/*"],
      "@layout/*": ["layout/*"],
      "@screens/*": ["screens/*"],
      "@context/*": ["context/*"],
      "@components/*": ["components/*"],
      "@utils/*": ["utils/*"]
    },
    "types": ["vite/client"]
  },
  "include": ["src"]
}


